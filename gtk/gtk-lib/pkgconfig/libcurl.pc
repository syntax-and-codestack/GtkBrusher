prefix=/mingw64
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
supported_protocols="DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS LDAP LDAPS MQTT POP3 POP3S RTSP SCP SFTP SMB SMBS SMTP SMTPS TELNET TFTP WS WSS"
supported_features="alt-svc AsynchDNS brotli HSTS HTTP2 HTTP3 HTTPS-proxy IDN IPv6 Kerberos Largefile libz NTLM PSL SPNEGO SSL SSPI threadsafe TLS-SRP UnixSockets zstd"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.12.1
Requires: 
Requires.private: libssh2,libidn2,openssl,zlib,libbrotlidec,libzstd,libnghttp2,libnghttp3,libpsl
Libs: -L${libdir} -lcurl 
Libs.private:  -lssh2 -lidn2 -lssl -lcrypto -lz -lbrotlidec -lbrotlicommon -lzstd -lnghttp2 -lnghttp3 -lwldap32 -lpsl -lws2_32 -lbcrypt -ladvapi32 -lcrypt32
Cflags: -I${includedir} 
Cflags.private: -DCURL_STATICLIB
